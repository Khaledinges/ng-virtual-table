{
  "name": "ng-virtual-table",
  "version": "1.0.16",
  "description": "Angular 7 virtual scroll table with support dynamic component, draggable, filtering, sorting, resizable and custom config column",
  "main": "index.js",
  "repository": "git@github.com:PxyUp/ng-virtual-table.git",
  "author": "Iurii Panarin <iurii.panarin@mcmakler.de>",
  "license": "MIT",
  "scripts": {
    "build": "ng-packagr -p package.json",
    "ct": "git add --all && git-cz",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "prepublish": "build",
    "localyarn": "yarn"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "./lib/public_api.ts"
    },
    "cssUrl": "inline",
    "whitelistedNonPeerDependencies": ["angular", "rxjs", "ng-dynamic-component"]
  },
  "peerDependencies": {
    "@angular/animations": "^7.0.1",
    "@angular/cdk": "^7.0.1",
    "@angular/cli": "^7.0.1",
    "@angular/common": "^7.0.1",
    "@angular/compiler": "^7.0.1",
    "@angular/compiler-cli": "^7.0.1",
    "@angular/core": "^7.0.1",
    "@angular/forms": "^7.0.1",
    "@angular/material": "^7.0.1",
    "@angular/platform-browser": "^7.0.1"
  },
  "devDependencies": {
    "@angular/animations": "^7.0.1",
    "@angular/cdk": "^7.0.1",
    "@angular/cli": "^7.0.1",
    "@angular/common": "^7.0.1",
    "@angular/compiler": "^7.0.1",
    "@angular/compiler-cli": "^7.0.1",
    "@angular/core": "^7.0.1",
    "@angular/forms": "^7.0.1",
    "@angular/material": "^7.0.1",
    "@angular/platform-browser": "^7.0.1",
    "@commitlint/config-conventional": "^7.1.2",
    "commitizen": "^3.0.4",
    "ng-packagr": "^4.3.1",
    "rxjs": "^6.3.3",
    "scss-bundle": "^2.4.0",
    "tsickle": "^0.33.0",
    "typescript": "^3.1.3",
    "yarn": "^1.10.1",
    "zone.js": "~0.8.26"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"],
    "rules": {
      "header-max-length": [2, "always", 80]
    }
  },
  "keywords": [
    "virtual scroll",
    "scroll",
    "ngx",
    "angular",
    "angular 7",
    "ngx",
    "dynamic",
    "component",
    "input",
    "output",
    "life-cycle"
  ],
  "dependencies": {
    "ng-dynamic-component": "^3.1.2"
  }
}